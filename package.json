{
  "private": true,
  "name": "shoppinglist",
  "description": "",
  "license": "MIT",
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint --cache --ext .ts,.tsx . --fix",
    "typecheck": "tsc",
    "build": "run-s compile:i18n build:css build:remix",
    "clean": "del .cache build public/build app/styles",
    "dev": "concurrently \"yarn:watch:css\" \"yarn:start:dev\"",
    "start": "run-s remix:setup remix:serve",
    "start:dev": "remix run",
    "build:remix": "remix build",
    "remix:setup": "remix setup",
    "remix:serve": "remix-serve build",
    "migrate": "prisma migrate deploy",
    "ts-node": "ts-node --compiler-options '{\"module\":\"CommonJS\"}'",
    "watch:css": "postcss styles --base styles --dir app/styles -w",
    "build:css": "postcss styles --base styles --dir app/styles --env production",
    "build:i18n": "run-s extract:i18n compile:i18n lint:fix",
    "extract:i18n": "formatjs extract 'app/**/*.ts*' --out-file app/lang/en-GB.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "compile:i18n": "formatjs compile app/lang/en-GB.json --ast --out-file app/lang/compiled/en-GB.json",
    "test": "run-s typecheck lint",
    "scalingo-postbuild": "run-s test build migrate remix:setup",
    "prebuild": "yarn clean",
    "postinstall": "run-s remix:setup"
  },
  "dependencies": {
    "@headlessui/react": "^1.4.1",
    "@heroicons/react": "^1.0.4",
    "@prisma/client": "^3.2.1",
    "@reach/alert": "^0.16.0",
    "@reach/combobox": "^0.16.1",
    "@reach/dialog": "^0.16.0",
    "@reach/disclosure": "^0.16.0",
    "@reach/tooltip": "^0.16.0",
    "@remix-run/node": "0.19.3",
    "@remix-run/react": "0.19.3",
    "@remix-run/serve": "0.19.3",
    "@sentry/react": "^6.13.3",
    "@sentry/tracing": "^6.13.3",
    "@tanem/react-nprogress": "^3.0.80",
    "argon2": "^0.28.2",
    "fp-ts": "^2.11.5",
    "fp-ts-contrib": "^0.1.26",
    "history": "^5.0.1",
    "hyper-ts": "^0.7.2",
    "hyper-ts-remix": "0.19.2",
    "interweave": "^12.7.2",
    "interweave-autolink": "^4.4.2",
    "interweave-ssr": "^1.2.1",
    "io-ts": "^2.2.16",
    "io-ts-types-experimental": "^0.3.0",
    "is-hotkey": "^0.2.0",
    "lru-cache": "^6.0.0",
    "luxon": "^2.0.2",
    "match-sorter": "^6.3.1",
    "ms": "^2.1.3",
    "npm-run-all": "^4.1.5",
    "prisma": "^3.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-intl": "^5.20.12",
    "react-router-dom": "6.0.0-beta.6",
    "react-spring": "^9.3.0",
    "react-use-clipboard": "^1.0.7",
    "react-use-gesture": "^9.1.3",
    "remix": "0.19.3",
    "resolve-accept-language": "^1.0.35",
    "use-debounce": "^7.0.0"
  },
  "devDependencies": {
    "@formatjs/cli": "^4.3.1",
    "@remix-run/dev": "0.19.3",
    "@tailwindcss/forms": "^0.3.4",
    "@tailwindcss/typography": "^0.4.1",
    "@types/is-hotkey": "^0.1.5",
    "@types/lru-cache": "^5.1.0",
    "@types/luxon": "^2.0.5",
    "@types/ms": "^0.7.31",
    "@types/node": "^16.11.0",
    "@types/react": "^17.0.30",
    "@types/react-dom": "^17.0.7",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "autoprefixer": "^10.3.7",
    "canvas": "^2.7.0",
    "concurrently": "^6.3.0",
    "del-cli": "^4.0.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-formatjs": "^2.17.8",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "postcss": "^8.3.9",
    "postcss-cli": "^8.3.1",
    "prettier": "^2.4.1",
    "tailwindcss": "^2.2.17",
    "ts-node": "^10.3.0",
    "typescript": "^4.4.4"
  },
  "prettier": {
    "singleQuote": true
  },
  "engines": {
    "node": "14",
    "npm": "7"
  },
  "sideEffects": false
}
