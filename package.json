{
  "private": true,
  "name": "shoppinglist-remix",
  "description": "",
  "license": "MIT",
  "scripts": {
    "build": "yarn build:css && yarn remix:build",
    "dev": "concurrently \"yarn:watch:css\" \"yarn:remix:run\"",
    "remix:run": "remix run",
    "remix:build": "remix build",
    "start": "remix-serve build",
    "migrate": "yarn prisma migrate deploy",
    "ts-node": "ts-node --compiler-options '{\"module\":\"CommonJS\"}'",
    "watch:css": "postcss styles --base styles --dir app/styles -w",
    "build:css": "postcss styles --base styles --dir app/styles --env production",
    "tsc": "tsc -b app/tsconfig.json",
    "scalingo-postbuild": "yarn build && yarn migrate",
    "postinstall": "if [ \"$INSTANCE_TYPE\" == \"production\" ]; then yarn build && yarn migrate; fi"
  },
  "dependencies": {
    "@headlessui/react": "^1.2.0",
    "@heroicons/react": "^1.0.0",
    "@prisma/client": "2.22.1",
    "@reach/alert": "^0.15.0",
    "@reach/combobox": "^0.15.1",
    "@reach/dialog": "^0.15.0",
    "@reach/disclosure": "^0.15.0",
    "@reach/tooltip": "^0.15.0",
    "@remix-run/node": "0.17.2",
    "@remix-run/react": "0.17.2",
    "@remix-run/serve": "0.17.2",
    "@sentry/react": "^6.3.6",
    "@sentry/tracing": "^6.3.6",
    "argon2": "^0.27.2",
    "fuse.js": "^6.4.6",
    "is-hotkey": "^0.2.0",
    "luxon": "^1.26.0",
    "ms": "^2.1.3",
    "prisma": "^2.22.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-intl": "^5.17.5",
    "react-router-dom": "^6.0.0-beta.0",
    "react-spring": "^9.1.2",
    "react-use-clipboard": "^1.0.7",
    "react-use-gesture": "^9.1.3",
    "remix": "0.17.2",
    "resolve-accept-language": "^1.0.16",
    "use-debounce": "^6.0.1",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@remix-run/dev": "0.17.2",
    "@tailwindcss/forms": "^0.3.2",
    "@tailwindcss/typography": "^0.4.0",
    "@types/is-hotkey": "^0.1.2",
    "@types/luxon": "^1.26.5",
    "@types/ms": "^0.7.31",
    "@types/node": "^15.0.3",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.4",
    "autoprefixer": "^10.2.5",
    "canvas": "^2.7.0",
    "concurrently": "^6.1.0",
    "postcss": "^8.2.15",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^2.1.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": "14"
  },
  "sideEffects": false
}
